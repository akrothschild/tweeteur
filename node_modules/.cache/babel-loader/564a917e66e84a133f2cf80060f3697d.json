,YAAA,CAAsD,OAAtD,EAAsD;AAA/C,EAAA,GAAG,EAAC;AAA2C,CAAtD,EAAoB,C,aAAC,YAAA,CAAyB,QAAzB,EAAyB,IAAzB,EAAQ,UAAR,CAAD,CAApB,E;;AAAA,C;;;AAGK,EAAA,KAAK,EAAC;;;8BACT,YAAA,CAAwD,OAAxD,EAAwD;AAAjD,EAAA,GAAG,EAAC;AAA6C,CAAxD,EAAwB,C,aAAC,YAAA,CAAuB,QAAvB,EAAuB,IAAvB,EAAQ,QAAR,CAAD,CAAxB,E;;AAAA,C;;+BAOF,YAAA,CAES,QAFT,EAES,IAFT,EAAQ,SAAR,EAEA,CAAA;AAAA;AAFA,C;;;AAME,EAAA,KAAK,EAAC;;;;;uBA5BZ,YAAA,CAuCM,KAvCN,EAAA,UAAA,EAuCM,CAtCL,YAAA,CA0BM,KA1BN,EAAA,UAAA,EA0BM,CAzBL,YAAA,CAA4D,IAA5D,EAAA,UAAA,EAAmC,MAAC,gBAAA,CAAG,KAAA,CAAA,IAAA,CAAK,QAAR,CAApC,EAAoD;AAAA;AAApD,GAyBK,EAvBwC,KAAA,CAAA,IAAA,CAAK,O,kBAAlD,YAAA,CAAsE,KAAtE,EAAA,UAAA,EAA2D,OAA3D,C,qCAuBK,EArBL,YAAA,CAEM,KAFN,EAAA,UAAA,EAEM,CADL,UACK,E,iBADuB,MAAC,gBAAA,CAAG,KAAA,CAAA,SAAH,C,EAAY;AAAA;GACpC,CAFN,CAqBK,EAjBL,YAAA,CAeO,MAfP,EAeO;AAfD,IAAA,KAAK,EAAC,2BAeL;AAfkC,IAAA,QAAM,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA;AAAA,aAAU,QAAA,CAAA,aAAA,OAAA,QAAA,YAAV;AAAA,KAAA,EAAuB,CAAA,SAAA,CAAvB,CAAA;AAexC,GAfP,E,CACE,U,kBACA,YAAA,CAAoE,UAApE,EAAoE;AAA1D,IAAA,EAAE,EAAC,SAAuD;AAA7C,IAAA,IAAI,EAAC,GAAwC;;aAA3B,KAAA,CAAA,cAAA,GAAc,M;;AAAa,GAApE,E,IAAA,E;;AAAA,G,iBAAyC,KAAA,CAAA,c,KAEzC,YAAA,CAOM,KAPN,EAAA,UAAA,EAOM,CANJ,UAMI,E,gBALJ,YAAA,CAIS,QAJT,EAIS;AAJD,IAAA,EAAE,EAAC,aAIF;;aAJyB,KAAA,CAAA,gBAAA,GAAgB,M;;AAIzC,GAJT,E,oBACE,YAAA,CAES,SAFT,EAES,IAFT,EAES,WAAA,CAF+C,KAAA,CAAA,SAE/C,EAFwD,UAA3B,MAA2B,EAAnB,KAAmB,EAAd;yBAAnD,YAAA,CAES,QAFT,EAES;AAFA,MAAA,KAAK,EAAE,MAAM,CAAC,KAEd;AAF2D,MAAA,GAAG,EAAE;AAEhE,KAFT,E,iBACK,MAAM,CAAC,I,CADZ,EACgB;AAAA;AADhB,MACgB,CAAA,OAAA,CADhB,C;GAES,CAFT,E;;AAAA,G,EADF,E;;AAAA,G,mBAAkC,KAAA,CAAA,gB,GAK9B,CAPN,C,EAQA,W,CAZF,E;;AAAA,GAiBK,CA1BN,CAsCK,EAXL,YAAA,CAUM,KAVN,EAAA,WAAA,EAUM,E,kBATL,YAAA,CAME,SANF,EAME,IANF,EAME,WAAA,CALc,KAAA,CAAA,IAAA,CAAK,KAKnB,EALwB,UAAlB,IAAkB,EAAd;yBADZ,YAAA,CAME,mBANF,EAME;AAJA,MAAA,GAAG,EAAE,IAAI,CAAC,EAIV;AAHA,MAAA,QAAQ,EAAE,KAAA,CAAA,IAAA,CAAK,QAGf;AAFA,MAAA,IAAI,EAAE,IAEN;AADA,MAAA,UAAQ,EAAE,QAAA,CAAA;AACV,KANF,E,IAAA,E;;AAAA,M,kCAAA,C;GAME,CANF,E;;AAAA,GASK,EAVN,CAWK,CAvCN,C","sourcesContent":["<template>\n <div class=\"user-profile\">\n  <div class=\"user-profile__user-panel\">\n   <h1 class=\"user-profile__username\">@{{ user.username }}</h1>\n\n   <div class=\"user-profile__admin-badge\" v-if=\"user.isAdmin\">Admin</div>\n   \n   <div class=\"user-profile__follower-count\">\n    <strong>Followers: </strong> {{ followers }}\n   </div>\n\n   <form class=\"user-profile__create-twit\" @submit.prevent=\"createNewTwit\">\n     <label for=\"newTwit\"><strong>New Twit</strong></label>\n     <textarea id=\"newTwit\" rows=\"4\" v-model=\"newTwitContent\"></textarea>\n\n     <div class=\"user-profile__create-twit-type\">\n       <label for=\"newTwitType\"><strong>Type: </strong></label>\n       <select id=\"newTwitType\" v-model=\"selectedTwitType\">\n         <option :value=\"option.value\" v-for=\"(option, index) in twitTypes\" :key=\"index\">\n           {{ option.name }}\n         </option>\n       </select>\n     </div>\n     <button>\n       Twit!\n     </button>\n   </form>\n\n  </div>\n  <div class=\"user-profile__twits-wrapper\">\n   <TwitItem \n    v-for=\"twit in user.twits\" \n    :key=\"twit.id\" \n    :username=\"user.username\" \n    :twit=\"twit\" \n    @favorite=\"toggleFavorite\"\n   />\n    \n  \n  </div>\n </div>\n</template>\n\n<script>\nimport TwitItem from \"./TwitItem\";\n\nexport default {\n name: 'UserProfile',\n components: { TwitItem },\n  data() {\n    return {\n      newTwitContent: '',\n      selectedTwitType: 'instant',\n      twitTypes: [\n        { value: 'draft', name: 'Draft'},\n        { value: 'instant', name: 'Instant Twit'}\n      ],\n      followers: 0,\n      user: {\n        id: 1,\n        username: '_RichardWindsor',\n        firstName: 'Richard',\n        lastName: 'Windsor',\n        email: 'rw@pm.me',\n        isAdmin: true,\n        twits: [ \n        \n         {\n          id: 1,\n          content: 'Twit is amazing!'\n         },\n         {\n          id: 2,\n          content: \"Subscribe today!\"\n         }\n        ]\n      }\n    }\n  },\n\n  watch: {\n    followers(newFollowerCount, oldFollowerCount) {\n      if (oldFollowerCount < newFollowerCount) {\n        console.log(`${this.user.username} has gained a follower!`)\n      }\n    }\n  },\n\n  computed: {\n    fullName() {\n      return `${this.user.firstName} ${this.user.lastName}`;\n    }\n  },\n  methods: {\n    followUser() {\n      this.followers++\n    },\n    toggleFavorite(id) {\n      console.log(`Favorited Twit #${id}`);\n    },\n    createNewTwit() {\n      if (this.newTwitContent && this.selectedTwitType !== 'draft') {this.user.twits.unshift (\n      {\n        id: (this.user.twits.length + 1),\n        content: this.newTwitContent,\n      })\n      this.newTwitContent = ''\n      }\n    }\n  },\n  mounted() {\n    this.followUser();\n  }\n}\n</script>\n\n<style lang= scoped>\n.user-profile {\n display: grid;\n grid-template-columns: 1fr 3fr;\n width: 100%;\n padding: 50px 5%;\n}\n\n.user-profile__user-panel {\n display: flex;\n flex-direction: column;\n margin-right: 50px;\n padding: 20px;\n background-color: white;\n border-radius: 5px;\n border: 1px solid #DFE3E8;\n}\n\n.user-profile__admin-badge {\n background: goldenrod;\n color: white;\n border-radius: 5px;\n margin-right: auto;\n padding: 0 10px;\n font-weight: bold;\n}\n\nh1 {\n margin: 0;\n}\n\n.user-profile__twits-wrapper {\n  display: grid;\n  grid-gap: 10 px;\n}\n\n.user-profile__create-twit {\n  border-top: 1px solid #DFE3E8;\n  padding-top: 20px;\n  display: flex;\n  flex-direction: column;\n}\n\n</style>\n\n"],"sourceRoot":""}]}