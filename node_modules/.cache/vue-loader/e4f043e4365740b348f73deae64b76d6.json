/TwitItem.vue","sourceRoot":"","sourcesContent":["<template>\n <div class=\"twit-item\" @click=\"favoriteTwit(twit.id)\">\n  <div class=\"user-profile__twit\">\n   <div class=\"twit-item__user\">\n    @{{ username }}\n   </div>\n   <div class=\"twit-item__content\">\n    {{ twit.content }}\n   </div>\n  </div>\n </div>\n</template>\n\n<script>\nexport default {\n name: \"TwitItem\",\n props: {\n\n  username: {\n   type: String,\n   required: true,\n  },\n \n  twit: {\n   type: Object,\n   required: true,\n  }\n },\n\n methods: {\n  favoriteTwit(id) {\n   this.$emit('favorite', id)\n  }\n }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.twit-item {\n padding: 20px;\n background-color: white;\n border-radius: 5px;\n border: 1px solid #DFE3E8;\n box-sizing: border-box;\n cursor: pointer;\n transition: all 0.25s ease;\n\n// .twit-item:hover\n &:hover { \n  transform: scale(1.1, 1.1);\n }\n\n  .twit-item__user {\n   font-weight: bold;\n  }\n}\n\n\n\n\n</style>"]}]}